{
  "name": "fastify-session-plus",
  "version": "1.0.0",
  "description": "Session plugin for fastify written in TypeScript supporting both stateless and stateful sessions. This is a fork with additional backward compatible features.",
  "author": "dmkng",
  "repository": "github:dmkng/fastify-session-plus",
  "license": "MIT",
  "access": "public",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run spec -- --watch",
    "build": "tsup src/index.ts --format cjs,esm --sourcemap --dts --clean",
    "lint": "eslint src/ test/",
    "prettycheck": "prettier --check src/ test/",
    "prettify": "prettier --write src/ test/",
    "typecheck": "tsc --noEmit",
    "spec": "vitest --run",
    "test": "npm run lint && npm run prettycheck && npm run typecheck && npm run spec",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "fastify-plugin": "^4.5.1",
    "nanoid": "^3.3.7"
  },
  "devDependencies": {
    "@fastify/cookie": "^9.3.1",
    "@mgcrea/eslint-config-node": "^0.10.1",
    "@mgcrea/fastify-session-redis-store": "^1.0.0",
    "@mgcrea/fastify-session-sodium-crypto": "^1.2.0",
    "@mgcrea/pino-pretty-compact": "^1.3.0",
    "@tsconfig/node-lts": "^20.1.3",
    "@tsconfig/strictest": "^2.0.5",
    "@types/benchmark": "^2.1.5",
    "@types/node": "^20.12.12",
    "@types/sodium-native": "^2.3.9",
    "benchmark": "^2.1.4",
    "eslint": "^8.57.0",
    "fastify": "^4.27.0",
    "ioredis": "^5.4.1",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "rimraf": "^5.0.7",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "fastify",
    "secure",
    "session",
    "sodium"
  ]
}
